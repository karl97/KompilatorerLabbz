
sum=0;
product = 1;
i=1;
read(n);
read(increment);
cett = 0;
ctva = 0;
ctre = 0;
cfour = 0;
while (i < n) {
    if (i % 1000 > 0) { cett=cett+1;} else {ctva=ctva+1; print(i); }
    ctre=ctre+1;
    sum = sum + i;
    product = product * i;
    i = i + 1;
    cfour = cfour + increment;
}
print(sum);
print(product);
print(cett);
print(ctva);
print(ctre);
print(cfour);

