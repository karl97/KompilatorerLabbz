
sum=0;
product = 1;
i=1;
read(n);
read(increment);
c1 = 0;
c2 = 0;
c3 = 0;
c4 = 0;
while (i < n) {
    if (i % 1000 > 0) { c1=c1+1;} else {c2=c2+1; print(i); }
    c3=c3+1;
    sum = sum + i;
    product = product * i;
    i = i + 1;
    c4 = c4 + increment;
}
print(sum);
print(product);
print(c1);
print(c2);
print(c3);
print(c4);
